lag(z_stringency_index, time_lag) + #fixed
(1 + lag(z_stringency_index, time_lag) | City) + # random
ar1( factor(time) + 0 | City) # autoregressive term
m6 <- glmmTMB(eq6,
dispformula = ~ 0,
REML = TRUE,
